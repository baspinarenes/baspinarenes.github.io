{
  "extends": [
    "eslint:recommended",
    "airbnb",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ],
    "react/jsx-props-no-spreading": [
      "error",
      {
        "custom": "ignore"
      }
    ]
  }
}
